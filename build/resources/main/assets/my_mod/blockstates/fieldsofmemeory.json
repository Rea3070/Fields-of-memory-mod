      "apply": {
        "model": "my_mod:block/fieldsofmemeory_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "my_mod:block/fieldsofmemeory_noside_alt"
      }
    },
    {
      "when": {
        "south": false
